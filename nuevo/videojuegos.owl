<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/videojuegos#"
     xml:base="http://www.semanticweb.org/videojuegos"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:dbr="http://dbpedia.org/resource/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/videojuegos"/>
    
    <!-- Clases -->
    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Videojuego">
        <rdfs:label>Videojuego</rdfs:label>
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/VideoGame"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Genero">
        <rdfs:label>Género</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Plataforma">
        <rdfs:label>Plataforma</rdfs:label>
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/InformationAppliance"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Desarrollador">
        <rdfs:label>Desarrollador</rdfs:label>
    </owl:Class>
    
    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#tieneGenero">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Genero"/>
        <rdfs:label>tiene género</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#tienePlataforma">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Plataforma"/>
        <rdfs:label>tiene plataforma</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#desarrolladoPor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Desarrollador"/>
        <rdfs:label>desarrollado por</rdfs:label>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/developer"/>
    </owl:ObjectProperty>
    
    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#titulo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>título</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#anioLanzamiento">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>año de lanzamiento</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#descripcion">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>descripción</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#dbpediaURI">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label>URI en DBpedia</rdfs:label>
    </owl:DatatypeProperty>
