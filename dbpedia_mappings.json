{
  "version": "1.0",
  "description": "Mapping between custom video game ontology and DBpedia",
  "lastUpdated": "2025-10-25",
  "classMappings": [
    {
      "local": "game:Juego",
      "dbpedia": "dbo:VideoGame",
      "equivalence": "owl:equivalentClass",
      "description": "Un videojuego corresponds exactly to DBpedia's VideoGame class"
    },
    {
      "local": "game:Desarrolladora",
      "dbpedia": "dbo:Company",
      "equivalence": "owl:equivalentClass",
      "description": "A game developer company"
    },
    {
      "local": "game:Editora",
      "dbpedia": "dbo:Company",
      "equivalence": "owl:equivalentClass",
      "description": "A game publisher company"
    },
    {
      "local": "game:Plataforma",
      "dbpedia": "dbo:Platform",
      "equivalence": "owl:equivalentClass",
      "description": "Gaming platform (console, PC, mobile)"
    },
    {
      "local": "game:Genero",
      "dbpedia": "dbo:Genre",
      "equivalence": "owl:equivalentClass",
      "description": "Video game genre"
    },
    {
      "local": "game:Premio",
      "dbpedia": "dbo:Award",
      "equivalence": "owl:equivalentClass",
      "description": "Video game award"
    }
  ],
  "propertyMappings": [
    {
      "local": "game:desarrolladoPor",
      "dbpedia": "dbo:developer",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "game:Desarrolladora"
    },
    {
      "local": "game:editadoPor",
      "dbpedia": "dbo:publisher",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "game:Editora"
    },
    {
      "local": "game:disponibleEn",
      "dbpedia": "dbo:platform",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "game:Plataforma"
    },
    {
      "local": "game:tieneGenero",
      "dbpedia": "dbo:genre",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "game:Genero"
    },
    {
      "local": "game:ganoPremio",
      "dbpedia": "dbo:award",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "game:Premio"
    },
    {
      "local": "game:fechaPublicacion",
      "dbpedia": "dbo:releaseDate",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "xsd:date"
    },
    {
      "local": "game:nombreJuego",
      "dbpedia": "rdfs:label",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "xsd:string"
    },
    {
      "local": "game:descripcionJuego",
      "dbpedia": "rdfs:comment",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Juego",
      "range": "xsd:string"
    },
    {
      "local": "game:anioFundacion",
      "dbpedia": "dbo:foundingYear",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Empresa",
      "range": "xsd:gYear"
    },
    {
      "local": "game:nombreEmpresa",
      "dbpedia": "rdfs:label",
      "equivalence": "owl:equivalentProperty",
      "domain": "game:Empresa",
      "range": "xsd:string"
    }
  ],
  "resourceLinks": [
    {
      "local": "game:Minecraft",
      "dbpedia": "http://dbpedia.org/resource/Minecraft",
      "relation": "owl:sameAs"
    },
