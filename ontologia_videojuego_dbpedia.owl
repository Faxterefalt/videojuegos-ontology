<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/game#"
     xml:base="http://example.org/game"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:game="http://example.org/game#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:dbr="http://dbpedia.org/resource/">
    
    <owl:Ontology rdf:about="http://example.org/game">
        <rdfs:label xml:lang="es">Ontología de Videojuegos</rdfs:label>
        <rdfs:label xml:lang="en">Video Game Ontology</rdfs:label>
        <rdfs:comment xml:lang="es">Ontología completa sobre videojuegos, compatible con DBpedia</rdfs:comment>
        <owl:imports rdf:resource="http://dbpedia.org/ontology/"/>
        <owl:versionInfo>2.0-DBpedia</owl:versionInfo>
    </owl:Ontology>

    <!-- ========== EQUIVALENCIAS DE PROPIEDADES ==========-->
    
    <owl:ObjectProperty rdf:about="http://example.org/game#desarrolladoPor">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Desarrolladora"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/developer"/>
        <rdfs:label xml:lang="es">Desarrollado Por</rdfs:label>
        <rdfs:label xml:lang="en">Developed By</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#editadoPor">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Editora"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/publisher"/>
        <rdfs:label xml:lang="es">Editado Por</rdfs:label>
        <rdfs:label xml:lang="en">Published By</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#disponibleEn">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Plataforma"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/platform"/>
        <rdfs:label xml:lang="es">Disponible En</rdfs:label>
        <rdfs:label xml:lang="en">Available On</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#tieneGenero">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Genero"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/genre"/>
        <rdfs:label xml:lang="es">Tiene Género</rdfs:label>
        <rdfs:label xml:lang="en">Has Genre</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#ganoPremio">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Premio"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/award"/>
        <rdfs:label xml:lang="es">Ganó Premio</rdfs:label>
        <rdfs:label xml:lang="en">Won Award</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#jugadoPor">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Participante"/>
        <rdfs:label xml:lang="es">Jugado Por</rdfs:label>
        <rdfs:label xml:lang="en">Played By</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#perteneceASaga">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Saga"/>
        <rdfs:label xml:lang="es">Pertenece A Saga</rdfs:label>
        <rdfs:label xml:lang="en">Part Of Series</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/game#incorporaTecnologia">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://example.org/game#Tecnologia"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/technology"/>
        <rdfs:label xml:lang="es">Incorpora Tecnología</rdfs:label>
        <rdfs:label xml:lang="en">Incorporates Technology</rdfs:label>
    </owl:ObjectProperty>

    <!-- ========== EQUIVALENCIAS DE PROPIEDADES DATATYPE ==========-->

    <owl:DatatypeProperty rdf:about="http://example.org/game#nombreJuego">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:equivalentProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
        <rdfs:label xml:lang="es">Nombre del Juego</rdfs:label>
        <rdfs:label xml:lang="en">Game Name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/game#descripcionJuego">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:equivalentProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <rdfs:label xml:lang="es">Descripción del Juego</rdfs:label>
        <rdfs:label xml:lang="en">Game Description</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/game#fechaPublicacion">
        <rdfs:domain rdf:resource="http://example.org/game#Juego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/releaseDate"/>
        <rdfs:label xml:lang="es">Fecha de Publicación</rdfs:label>
        <rdfs:label xml:lang="en">Release Date</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/game#nombreEmpresa">
        <rdfs:domain rdf:resource="http://example.org/game#Empresa"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:equivalentProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
        <rdfs:label xml:lang="es">Nombre de la Empresa</rdfs:label>
        <rdfs:label xml:lang="en">Company Name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/game#anioFundacion">
        <rdfs:domain rdf:resource="http://example.org/game#Empresa"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/foundingYear"/>
        <rdfs:label xml:lang="es">Año de Fundación</rdfs:label>
        <rdfs:label xml:lang="en">Founding Year</rdfs:label>
    </owl:DatatypeProperty>

    <!-- ========== EQUIVALENCIAS DE CLASES ==========-->

    <owl:Class rdf:about="http://example.org/game#Juego">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/VideoGame"/>
        <rdfs:label xml:lang="es">Juego</rdfs:label>
        <rdfs:label xml:lang="en">Video Game</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.org/game#desarrolladoPor"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/game#Desarrolladora">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Company"/>
        <rdfs:subClassOf rdf:resource="http://example.org/game#Empresa"/>
        <rdfs:label xml:lang="es">Desarrolladora</rdfs:label>
        <rdfs:label xml:lang="en">Developer</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/game#Editora">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Company"/>
        <rdfs:subClassOf rdf:resource="http://example.org/game#Empresa"/>
        <rdfs:label xml:lang="es">Editora</rdfs:label>
        <rdfs:label xml:lang="en">Publisher</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/game#Plataforma">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Platform"/>
        <rdfs:label xml:lang="es">Plataforma</rdfs:label>
        <rdfs:label xml:lang="en">Platform</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/game#Genero">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Genre"/>
        <rdfs:subClassOf rdf:resource="http://example.org/game#Juego"/>
        <rdfs:label xml:lang="es">Género</rdfs:label>
        <rdfs:label xml:lang="en">Genre</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/game#Premio">
        <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Award"/>
        <rdfs:label xml:lang="es">Premio</rdfs:label>
        <rdfs:label xml:lang="en">Award</rdfs:label>
    </owl:Class>

    <!-- ========== INDIVIDUOS CON OWL:SAMEAS LINKS A DBPEDIA ==========-->

    <owl:NamedIndividual rdf:about="http://example.org/game#MineCraft">
        <rdf:type rdf:resource="http://example.org/game#Juego"/>
        <desarrolladoPor rdf:resource="http://example.org/game#Mojang"/>
        <nombreJuego>Minecraft</nombreJuego>
        <fechaPublicacion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2011-11-18</fechaPublicacion>
        <descripcionJuego>Juego de construcción y supervivencia</descripcionJuego>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Minecraft"/>
        <rdfs:label xml:lang="es">Minecraft</rdfs:label>
        <rdfs:label xml:lang="en">Minecraft</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#CallOfDuty">
        <rdf:type rdf:resource="http://example.org/game#Juego"/>
        <desarrolladoPor rdf:resource="http://example.org/game#Activision"/>
        <nombreJuego>Call of Duty: Modern Warfare</nombreJuego>
        <fechaPublicacion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-10-25</fechaPublicacion>
        <descripcionJuego>Shooter en primera persona</descripcionJuego>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Call_of_Duty:_Modern_Warfare"/>
        <rdfs:label xml:lang="es">Call of Duty</rdfs:label>
        <rdfs:label xml:lang="en">Call of Duty</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#CyberPunk2077">
        <rdf:type rdf:resource="http://example.org/game#Juego"/>
        <desarrolladoPor rdf:resource="http://example.org/game#CDProjektRed"/>
        <nombreJuego>Cyberpunk 2077</nombreJuego>
        <fechaPublicacion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-12-10</fechaPublicacion>
        <descripcionJuego>RPG futurista de mundo abierto</descripcionJuego>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Cyberpunk_2077"/>
        <rdfs:label xml:lang="es">Cyberpunk 2077</rdfs:label>
        <rdfs:label xml:lang="en">Cyberpunk 2077</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#LeagueOfLegends">
        <rdf:type rdf:resource="http://example.org/game#Juego"/>
        <desarrolladoPor rdf:resource="http://example.org/game#RiotGames"/>
        <nombreJuego>League of Legends</nombreJuego>
        <fechaPublicacion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2009-10-27</fechaPublicacion>
        <descripcionJuego>MOBA multijugador</descripcionJuego>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/League_of_Legends"/>
        <rdfs:label xml:lang="es">League of Legends</rdfs:label>
        <rdfs:label xml:lang="en">League of Legends</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#TheWitcher3">
        <rdf:type rdf:resource="http://example.org/game#Juego"/>
        <desarrolladoPor rdf:resource="http://example.org/game#Activision"/>
        <nombreJuego>The Witcher 3: Wild Hunt</nombreJuego>
        <fechaPublicacion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-05-19</fechaPublicacion>
        <descripcionJuego>RPG de mundo abierto</descripcionJuego>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/The_Witcher_3:_Wild_Hunt"/>
        <rdfs:label xml:lang="es">The Witcher 3</rdfs:label>
        <rdfs:label xml:lang="en">The Witcher 3</rdfs:label>
    </owl:NamedIndividual>

    <!-- ========== EMPRESAS ENLAZADAS CON DBPEDIA ==========-->

    <owl:NamedIndividual rdf:about="http://example.org/game#Nintendo">
        <rdf:type rdf:resource="http://example.org/game#Desarrolladora"/>
        <nombreEmpresa>Nintendo</nombreEmpresa>
        <anioFundacion rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">1889</anioFundacion>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Nintendo"/>
        <rdfs:label xml:lang="es">Nintendo</rdfs:label>
        <rdfs:label xml:lang="en">Nintendo</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#Activision">
        <rdf:type rdf:resource="http://example.org/game#Desarrolladora"/>
