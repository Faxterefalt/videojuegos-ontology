        <rdfs:label xml:lang="en">Mojang Studios</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#RiotGames">
        <rdf:type rdf:resource="http://example.org/game#Desarrolladora"/>
        <nombreEmpresa>Riot Games</nombreEmpresa>
        <anioFundacion rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">2006</anioFundacion>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Riot_Games"/>
        <rdfs:label xml:lang="es">Riot Games</rdfs:label>
        <rdfs:label xml:lang="en">Riot Games</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#CDProjektRed">
        <rdf:type rdf:resource="http://example.org/game#Desarrolladora"/>
        <nombreEmpresa>CD Projekt Red</nombreEmpresa>
        <anioFundacion rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">2002</anioFundacion>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/CD_Projekt_Red"/>
        <rdfs:label xml:lang="es">CD Projekt Red</rdfs:label>
        <rdfs:label xml:lang="en">CD Projekt Red</rdfs:label>
    </owl:NamedIndividual>

    <!-- ========== PLATAFORMAS ENLAZADAS CON DBPEDIA ==========-->

    <owl:NamedIndividual rdf:about="http://example.org/game#PlayStation5">
        <rdf:type rdf:resource="http://example.org/game#Consola"/>
        <nombrePlataforma>PlayStation 5</nombrePlataforma>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/PlayStation_5"/>
        <rdfs:label xml:lang="es">PlayStation 5</rdfs:label>
        <rdfs:label xml:lang="en">PlayStation 5</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#XboxSeriesX">
        <rdf:type rdf:resource="http://example.org/game#Consola"/>
        <nombrePlataforma>Xbox Series X</nombrePlataforma>
        <owl:sameAs rdf:resource="http://dbpedia.org/resource/Xbox_Series_X_and_Series_S"/>
        <rdfs:label xml:lang="es">Xbox Series X</rdfs:label>
        <rdfs:label xml:lang="en">Xbox Series X</rdfs:label>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/game#NintendoSwitch2">
