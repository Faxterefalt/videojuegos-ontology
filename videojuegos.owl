<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/videojuegos#"
     xml:base="http://www.semanticweb.org/videojuegos"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vg="http://www.semanticweb.org/videojuegos#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/videojuegos">
        <rdfs:label xml:lang="es">Ontología de Videojuegos</rdfs:label>
        <rdfs:label xml:lang="en">Video Games Ontology</rdfs:label>
        <rdfs:comment xml:lang="es">Una ontología completa para describir videojuegos, géneros, plataformas, desarrolladores y otros aspectos del dominio de los videojuegos.</rdfs:comment>
        <rdfs:comment xml:lang="en">A comprehensive ontology for describing video games, genres, platforms, developers and other aspects of the video game domain.</rdfs:comment>
        <owl:versionInfo>1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/videojuegos#Videojuego -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Videojuego">
        <rdfs:label xml:lang="es">Videojuego</rdfs:label>
        <rdfs:label xml:lang="en">Video Game</rdfs:label>
        <rdfs:comment xml:lang="es">Un juego electrónico que involucra la interacción con una interfaz de usuario para generar retroalimentación visual en un dispositivo de video.</rdfs:comment>
        <rdfs:comment xml:lang="en">An electronic game that involves interaction with a user interface to generate visual feedback on a video device.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#Genero -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Genero">
        <rdfs:label xml:lang="es">Género</rdfs:label>
        <rdfs:label xml:lang="en">Genre</rdfs:label>
        <rdfs:comment xml:lang="es">Categoría de videojuegos basada en el estilo de juego, mecánicas y características comunes.</rdfs:comment>
        <rdfs:comment xml:lang="en">Category of video games based on gameplay style, mechanics and common characteristics.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#Plataforma -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Plataforma">
        <rdfs:label xml:lang="es">Plataforma</rdfs:label>
        <rdfs:label xml:lang="en">Platform</rdfs:label>
        <rdfs:comment xml:lang="es">Sistema hardware o software en el cual se ejecuta un videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">Hardware or software system on which a video game runs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#Desarrollador -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Desarrollador">
        <rdfs:label xml:lang="es">Desarrollador</rdfs:label>
        <rdfs:label xml:lang="en">Developer</rdfs:label>
        <rdfs:comment xml:lang="es">Persona o empresa responsable del desarrollo de un videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">Person or company responsible for developing a video game.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#Editor -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Editor">
        <rdfs:label xml:lang="es">Editor</rdfs:label>
        <rdfs:label xml:lang="en">Publisher</rdfs:label>
        <rdfs:comment xml:lang="es">Empresa responsable de la publicación y distribución de un videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">Company responsible for publishing and distributing a video game.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#Jugador -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#Jugador">
        <rdfs:label xml:lang="es">Jugador</rdfs:label>
        <rdfs:label xml:lang="en">Player</rdfs:label>
        <rdfs:comment xml:lang="es">Persona que juega videojuegos.</rdfs:comment>
        <rdfs:comment xml:lang="en">Person who plays video games.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#ModoJuego -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#ModoJuego">
        <rdfs:label xml:lang="es">Modo de Juego</rdfs:label>
        <rdfs:label xml:lang="en">Game Mode</rdfs:label>
        <rdfs:comment xml:lang="es">Diferentes formas de jugar un videojuego (individual, multijugador, cooperativo, etc.).</rdfs:comment>
        <rdfs:comment xml:lang="en">Different ways to play a video game (single-player, multiplayer, cooperative, etc.).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/videojuegos#ClasificacionEdad -->

    <owl:Class rdf:about="http://www.semanticweb.org/videojuegos#ClasificacionEdad">
        <rdfs:label xml:lang="es">Clasificación por Edad</rdfs:label>
        <rdfs:label xml:lang="en">Age Rating</rdfs:label>
        <rdfs:comment xml:lang="es">Sistema de clasificación que indica la edad apropiada para jugar un videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">Rating system that indicates the appropriate age to play a video game.</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/videojuegos#tieneGenero -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#tieneGenero">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Genero"/>
        <rdfs:label xml:lang="es">tiene género</rdfs:label>
        <rdfs:label xml:lang="en">has genre</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un videojuego con su género.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a video game to its genre.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#disponibleEn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#disponibleEn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Plataforma"/>
        <rdfs:label xml:lang="es">disponible en</rdfs:label>
        <rdfs:label xml:lang="en">available on</rdfs:label>
        <rdfs:comment xml:lang="es">Indica en qué plataformas está disponible un videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates on which platforms a video game is available.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#desarrolladoPor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#desarrolladoPor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Desarrollador"/>
        <rdfs:label xml:lang="es">desarrollado por</rdfs:label>
        <rdfs:label xml:lang="en">developed by</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un videojuego con su desarrollador.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a video game to its developer.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#publicadoPor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#publicadoPor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Editor"/>
        <rdfs:label xml:lang="es">publicado por</rdfs:label>
        <rdfs:label xml:lang="en">published by</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un videojuego con su editor.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a video game to its publisher.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#tieneModoJuego -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#tieneModoJuego">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#ModoJuego"/>
        <rdfs:label xml:lang="es">tiene modo de juego</rdfs:label>
        <rdfs:label xml:lang="en">has game mode</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un videojuego con sus modos de juego disponibles.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a video game to its available game modes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#tieneClasificacion -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#tieneClasificacion">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#ClasificacionEdad"/>
        <rdfs:label xml:lang="es">tiene clasificación</rdfs:label>
        <rdfs:label xml:lang="en">has rating</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un videojuego con su clasificación por edad.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a video game to its age rating.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#juega -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/videojuegos#juega">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Jugador"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:label xml:lang="es">juega</rdfs:label>
        <rdfs:label xml:lang="en">plays</rdfs:label>
        <rdfs:comment xml:lang="es">Relaciona un jugador con los videojuegos que juega.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a player to the video games they play.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/videojuegos#titulo -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#titulo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="es">título</rdfs:label>
        <rdfs:label xml:lang="en">title</rdfs:label>
        <rdfs:comment xml:lang="es">El título o nombre del videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">The title or name of the video game.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#fechaLanzamiento -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#fechaLanzamiento">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:label xml:lang="es">fecha de lanzamiento</rdfs:label>
        <rdfs:label xml:lang="en">release date</rdfs:label>
        <rdfs:comment xml:lang="es">La fecha en que fue lanzado el videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">The date when the video game was released.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#descripcion -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#descripcion">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="es">descripción</rdfs:label>
        <rdfs:label xml:lang="en">description</rdfs:label>
        <rdfs:comment xml:lang="es">Una descripción del videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">A description of the video game.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#precio -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#precio">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:label xml:lang="es">precio</rdfs:label>
        <rdfs:label xml:lang="en">price</rdfs:label>
        <rdfs:comment xml:lang="es">El precio del videojuego.</rdfs:comment>
        <rdfs:comment xml:lang="en">The price of the video game.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#puntuacion -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#puntuacion">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/videojuegos#Videojuego"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:label xml:lang="es">puntuación</rdfs:label>
        <rdfs:label xml:lang="en">score</rdfs:label>
        <rdfs:comment xml:lang="es">La puntuación promedio del videojuego (0-10).</rdfs:comment>
        <rdfs:comment xml:lang="en">The average score of the video game (0-10).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/videojuegos#nombre -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/videojuegos#nombre">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#Desarrollador"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#Editor"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#Jugador"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#Genero"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#Plataforma"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#ModoJuego"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/videojuegos#ClasificacionEdad"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="es">nombre</rdfs:label>
        <rdfs:label xml:lang="en">name</rdfs:label>
        <rdfs:comment xml:lang="es">El nombre de la entidad.</rdfs:comment>
        <rdfs:comment xml:lang="en">The name of the entity.</rdfs:comment>
    </owl:DatatypeProperty>
    


</rdf:RDF>